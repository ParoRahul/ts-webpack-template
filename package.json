{
  "name": "webpack_ts_template",
  "version": "1.0.0",
  "description": "WebPack Typescript template ",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParoRahul/ts-webpack-template.git"
  },
  "keywords": [
    "webpack",
    "Typescript"
  ],
  "engines": {
    "node": ">= 10",
    "npm": ">= 1.6"
  },
  "scripts": {
    "serve": "webpack serve --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\"  mocha -r ts-node/register test/**/*.test.ts",
    "test:report": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\"  mocha --reporter mochawesome -r ts-node/register test/**/*.test.ts",
    "version": "webpack --version",
    "lint": "eslint  --ext .ts .",
    "lint:fix": "eslint --ext .ts  --fix ."
  },
  "author": "Rahul Das",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "bootstrap": "^4.5.3",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "material-icons": "^0.5.2",
    "mini-css-extract-plugin": "^1.3.4",
    "mocha": "^8.2.1",
    "mochawesome": "^6.2.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {}
}
